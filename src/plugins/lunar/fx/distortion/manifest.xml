<?xml version="1.0" encoding="utf-8"?>
<zzub>
  <plugin
     uri="@trac.zeitherrschaft.org/aldrin/lunar/effect/distortion;1"
     name="Lunar Distortion"
     shortname="Distortion"
     type="effect"
     description="An exponential distortion plug-in."
     author="Vytautas JanÄauskas"
     email="unaudio@gmail.com">
    <parameters>
      <valuenamegroup>
	<valuenames id="algs">
	  <alias value="0" name="Smooth"/>
	  <alias value="1" name="Hard"/>
	  <alias value="2" name="Sine"/>
	  <alias value="3" name="Fold"/>
	  <alias value="4" name="Trash"/>
	</valuenames>
      </valuenamegroup>
      <global>
	<parameter
	   id="alg"
	   name="Type"
	   description="Select distortion type"
	   type="byte"
	   minvalue="0"
	   maxvalue="4"
	   defvalue="0"
	   valuenames="algs"
	   state="true"/>
	<parameter
	   id="pregain"
	   name="Gain In (dB)"
	   description="Gain of input signal"
	   type="float"
	   minvalue="0"
	   maxvalue="24"
	   defvalue="0"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="postgain"
	   name="Gain Out (dB)"
	   description="Gain of output signal"
	   type="float"
	   minvalue="-24"
	   maxvalue="0"
	   defvalue="0"
	   precision="0.01"
	   state="true"/>
      </global>
      <track>
      </track>
    </parameters>
    <files>
      <file ref="distortion.cpp"/>
      <file ref="distortion.h"/>
    </files>
    <modules>
      <module id="distortion">
	<source language="c++" ref="distortion.cpp" fxdef="distortion.h"/>
      </module>
    </modules>
  </plugin>
</zzub>
