<?xml version="1.0" encoding="utf-8"?>
<zzub>
  <plugin
     uri="@trac.zeitherrschaft.org/aldrin/lunar/effect/distortion;1"
     name="Lunar Distortion"
     shortname="Distortion"
     type="effect"
     description="An exponential distortion plug-in."
     author="Vytautas JanÄauskas"
     email="unaudio@gmail.com">
    <parameters>
      <global>
	<parameter
	   id="pregain"
	   name="Gain In (dB)"
	   description="Gain of input signal"
	   type="float"
	   minvalue="-12"
	   maxvalue="32"
	   defvalue="0"
	   precision="0.01"
	   state="true"/>
	<parameter
	   id="asymmetry"
	   name="Assymetry"
	   description="Assymetry coefficient."
	   type="float"
	   minvalue="1.0"
	   maxvalue="1.2"
	   defvalue="1.0"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="rectification"
	   name="Rectification"
	   description="Rectification parameter (DC offset)."
	   type="float"
	   minvalue="0.0"
	   maxvalue="1.0"
	   defvalue="0.0"
	   precision="0.001"
	   state="true"/>
	<parameter
	   id="postgain"
	   name="Gain Out (dB)"
	   description="Gain of output signal"
	   type="float"
	   minvalue="-32"
	   maxvalue="12"
	   defvalue="0"
	   precision="0.01"
	   state="true"/>
      </global>
      <track>
      </track>
    </parameters>
    <files>
      <file ref="distortion.cpp"/>
      <file ref="distortion.h"/>
    </files>
    <modules>
      <module id="distortion">
	<source language="c++" ref="distortion.cpp" fxdef="distortion.h"/>
      </module>
    </modules>
  </plugin>
</zzub>
